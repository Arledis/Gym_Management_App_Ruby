<h1>Bookings</h1>



  <table id="bookings">
    <tr>
      <th>Client:</th>
      <th>Class:</th>
      <th>Date & Time:</th>
      <th>Booking ID:</th>
    </tr>
        <% for booking in @bookings %>
    <tr>
      <td><%= booking.client.name %></td>
      <td><%=booking.gymclass.name %></td>
      <td><%=booking.gymclass.schedule %></td>
      <td><%=booking.id %></td>
      <td><form action="/bookings/<%= booking.id %>/delete" method="POST">
        <input class="btn btn--delete" type="submit" value="Delete" />
      </form></td>
    </tr>

  <% end %>
  </table>
